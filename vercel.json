{
    "builds": [
        {
            "src": "main/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        }
    ],

    "rewrites":[
        { "source":"/(.*)", "destination": "/" }
    ],

    "roures": [
        {
            "src": "/(.*)",
            "dest": "main/wsgi.py"
        }
    ]
}